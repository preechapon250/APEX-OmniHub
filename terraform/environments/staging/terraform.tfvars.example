# Copy this file to terraform.tfvars and fill in your values
# NEVER commit terraform.tfvars to git!

# Provider credentials
cloudflare_api_token = "your-cloudflare-token"
cloudflare_zone_id   = "your-zone-id"
upstash_email        = "your-email@example.com"
upstash_api_key      = "your-upstash-key"
vercel_token         = "your-vercel-token"

# Application
github_repo = "apexbusiness-systems/OmniLink-APEX"

# Environment variables (from Doppler)
vite_supabase_url                = "https://staging.supabase.co"
vite_supabase_publishable_key    = "your-staging-anon-key"
vite_sentry_dsn                  = "https://...@sentry.io/..."
vite_datadog_application_id      = "your-datadog-app-id"
vite_datadog_client_token        = "your-datadog-token"
