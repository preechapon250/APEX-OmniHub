{
  "@context": {
    "@version": 1.1,
    "@base": "https://apex-omnilink.com/vocab#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "eventId": {
      "@id": "eventId",
      "@type": "xsd:string"
    },
    "correlationId": {
      "@id": "correlationId",
      "@type": "xsd:string"
    },
    "tenantId": {
      "@id": "tenantId",
      "@type": "xsd:string"
    },
    "userId": {
      "@id": "userId",
      "@type": "xsd:string"
    },
    "source": {
      "@id": "source",
      "@type": "xsd:string"
    },
    "provider": {
      "@id": "provider",
      "@type": "xsd:string"
    },
    "externalId": {
      "@id": "externalId",
      "@type": "xsd:string"
    },
    "eventType": {
      "@id": "eventType",
      "@type": "xsd:string"
    },
    "timestamp": {
      "@id": "timestamp",
      "@type": "xsd:dateTime"
    },
    "locale": {
      "@id": "locale",
      "@type": "xsd:string",
      "@pattern": "^(en|es|de|ja|fr|pt|it)$"
    },
    "consentFlags": {
      "@id": "consentFlags",
      "@type": "@id"
    },
    "metadata": {
      "@id": "metadata",
      "@type": "@id"
    },
    "payload": {
      "@id": "payload",
      "@type": "@id"
    },
    "CanonicalEvent": {
      "@id": "CanonicalEvent",
      "@type": "@id"
    }
  }
}