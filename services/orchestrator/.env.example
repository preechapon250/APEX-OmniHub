# Temporal Configuration
TEMPORAL_HOST=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=apex-orchestrator

# Redis Configuration (Upstash)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_SSL=false

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_DB_URL=postgresql://postgres:password@localhost:54322/postgres

# LLM Configuration
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
DEFAULT_LLM_MODEL=gpt-4-turbo-preview
DEFAULT_LLM_TEMPERATURE=0.0

# Semantic Cache Configuration
CACHE_EMBEDDING_MODEL=all-MiniLM-L6-v2
CACHE_SIMILARITY_THRESHOLD=0.85
CACHE_TTL_SECONDS=86400

# Application Configuration
LOG_LEVEL=INFO
ENVIRONMENT=development
ENABLE_DISTRIBUTED_LOCKS=true
MAX_WORKFLOW_HISTORY_SIZE=1000
